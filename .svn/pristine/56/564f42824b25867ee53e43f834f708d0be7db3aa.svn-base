 * @author 208396 - EVANDRO CUSTODIO GONCALVES
 *
 */
public class BtpProjeto
{

	private String id;

	private String nome;

	private String plano;

	private String build;

	private Integer reteste;

	private Integer execucao;

	private Boolean reportar;

	public BtpProjeto(String nome, String plano, String build)
	{
		super();
		this.nome = nome;
		this.plano = plano;
		this.build = build;
	}

	public BtpProjeto()
	{
	}

	public String getNome()
	{
		return nome;
	}

	public void setNome(String nome)
	{
		this.nome = nome;
	}

	public String getPlano()
	{
		return plano;
	}

	public void setPlano(String plano)
	{
		this.plano = plano;
	}

	public String getBuild()
	{
		return build;
	}

	public void setBuild(String build)
	{
		this.build = build;
	}

	public String getId()
	{
		return id;
	}

	public void setId(String id)
	{
		this.id = id;
	}

	public Integer getReteste()
	{
		return reteste;
	}

	public void setReteste(Integer reteste)
	{
		this.reteste = reteste;
	}

	public EnumTipoExecucao getExecucao()
	{
		return EnumTipoExecucao.valueOf(execucao);
	}

	public void setExecucao(Integer execucao)
	{
		this.execucao = execucao;
	}

	public void setReportar(Boolean reportar)
	{
		this.reportar = reportar;		
	}
	
	public Boolean getReportar()
	{
		return reportar;
	}

}
